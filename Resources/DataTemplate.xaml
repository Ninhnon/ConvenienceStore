<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:view="clr-namespace:ConvenienceStore.Views"
                    xmlns:vm="clr-namespace:ConvenienceStore.ViewModel"
                    xmlns:vmLam="clr-namespace:ConvenienceStore.ViewModel.Lam"
                    >

    <DataTemplate DataType="{x:Type vm:HomeViewModel}">
        <view:HomeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:EmployeeViewModel}">
        <view:EmployeeView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vmLam:InputInfoVM}">
        <view:InputInfoView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ProfileViewModel}">
        <view:ProfileView/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:ReportViewModel}">
        <view:ReportView/>
    </DataTemplate>
</ResourceDictionary>